{
  "name": "leapp-shellcreds",
  "author": "Taimos GmbH",
  "version": "0.0.1",
  "description": "Plugin to copy AWS credentials in a shell compatible format",
  "keywords": [
    "leapp-plugin"
  ],
  "leappPlugin": {
    "supportedOS": [
      "mac",
      "windows",
      "linux"
    ],
    "supportedSessions": [
      "awsIamRoleFederated",
      "awsIamRoleChained",
      "awsSsoRole",
      "awsIamUser",
      "aws"
    ]
  },
  "scripts": {
    "build": "webpack --config webpack.config.js"
  },
  "files": [
    "plugin.js"
  ],
  "license": "MIT",
  "dependencies": {
    "@noovolari/leapp-core": "^0.1.117",
    "clipboardy": "^3.0.0"
  },
  "devDependencies": {
    "path-browserify": "^1.0.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
